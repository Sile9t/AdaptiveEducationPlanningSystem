{
    "openapi": "3.0.0",
    "info": {
        "title": "Authentication controller",
        "version": "1.0.0"
    },
    "paths": {
        "/login": {
            "post": {
                "summary": "Login a user",
                "operationId": "42f77e072dec84b0e1094cca1228298e",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "email": {
                                        "type": "string"
                                    },
                                    "password": {
                                        "type": "string"
                                    },
                                    "remember": {
                                        "type": "boolean"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "302": {
                        "description": "Redirect"
                    },
                    "400": {
                        "description": "Bad request"
                    }
                }
            }
        },
        "/logout": {
            "post": {
                "summary": "Logout a user",
                "operationId": "03afb12b342c2ea43bdc1de4f39e91a8",
                "requestBody": {},
                "responses": {
                    "302": {
                        "description": "Redirect"
                    }
                }
            }
        }
    }
}